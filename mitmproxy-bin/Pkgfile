# Description: SSL-capable man-in-the-middle HTTP proxy
# URL:         https://mitmproxy.org/
# Depends on:  zlib

name=mitmproxy-bin
version=12.2.1
release=1
source=https://downloads.mitmproxy.org/$version/mitmproxy-$version-linux-x86_64.tar.gz

build() {
	ls -la .
	mkdir -p $PKG/usr/bin
	mv mitmdump mitmproxy mitmweb $PKG/usr/bin/
}
