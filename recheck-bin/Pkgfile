# Description: Trustworthy ReDoS checker
# URL:         https://github.com/makenowjust-labs/recheck
# Depends on:  zlib

name=recheck-bin
version=4.5.0-beta.4
release=1
source=$name-$version-amd64.bin.xz

build() {
	xz -d $name-$version-amd64.bin.xz --threads=$JOBS
	ls
	install -m 0755 -D $name-$version-amd64.bin \
		$PKG/usr/bin/recheck
}
